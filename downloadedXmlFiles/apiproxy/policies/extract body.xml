<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="true" enabled="false" name="extract body">
    <DisplayName>extract body</DisplayName>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source>request</Source>
    <JSONPayload>
        <Variable name="username">
            <JSONPath>$.username</JSONPath>
        </Variable>
        <Variable name="password">
            <JSONPath>$.password</JSONPath>
        </Variable>
        <Variable name="mobile">
            <JSONPath>$.mobile</JSONPath>
        </Variable>
        <Variable name="message">
            <JSONPath>$.message</JSONPath>
        </Variable>
        <Variable name="sendername">
            <JSONPath>$.sendername</JSONPath>
        </Variable>
    </JSONPayload>
</ExtractVariables>